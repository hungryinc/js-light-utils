  "repository": {
    "type": "git",
    "url": "git+https://github.com/hungryinc/js-light-utils.git"
  },
  "license": "MIT",
  "scripts": {
    "build-tests": "browserify ./tests/test.js > ./tests/main.js"
  },
  "bugs": {
    "url": "https://github.com/hungryinc/js-light-utils/issues"
  },
  "homepage": "https://github.com/hungryinc/js-light-utils#readme",
